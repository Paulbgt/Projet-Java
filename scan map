			public void ScanMap(String[][] map){
				for(int y = 30 ; y != -1 ; y -- ){
					for(int x = 40 ; x != -1 ; x -- ){
						
						if(map[x][y] == "S"){
							Stone.StoneMoove(map, x, y);
						}
						
						if(map[x][y] == "D"){
							Diamond.DiamondMoove(map, x , y);
						}
						
						if(map[x][y] == "M"){
							Monster.MonsterMoove(map, x , y);
						}
					}
				}
			}
